<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="toplevel1">
    <property name="height">200</property>
    <property name="padx">10</property>
    <property name="pady">10</property>
    <property name="title" translatable="yes">Panther Pantry Employee Login</property>
    <property name="width">200</property>
    <containerlayout manager="grid">
      <property type="col" id="0" name="weight">1</property>
      <property type="col" id="all" name="weight">1</property>
      <property type="row" id="1" name="weight">1</property>
      <property type="row" id="all" name="weight">1</property>
    </containerlayout>
    <child>
      <object class="ttk.Label" id="login_label" named="True">
        <property name="font">{Times New Roman} 16 {}</property>
        <property name="text" translatable="yes">Welcome to the Food Pantry</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="frame1">
        <property name="height">200</property>
        <property name="padding">10</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">1</property>
          <property name="sticky">ew</property>
        </layout>
        <containerlayout manager="grid">
          <property type="col" id="0" name="weight">1</property>
          <property type="col" id="all" name="weight">1</property>
          <property type="row" id="0" name="weight">1</property>
          <property type="row" id="1" name="weight">1</property>
          <property type="row" id="all" name="weight">1</property>
        </containerlayout>
        <child>
          <object class="ttk.Label" id="login_greeting" named="True">
            <property name="font">{Times New Roman} 14 {}</property>
            <property name="justify">left</property>
            <property name="text" translatable="yes">Log in here</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="login_message" named="True">
            <property name="font">{Times New Roman} 12 {}</property>
            <property name="text" translatable="yes">Account Login:</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Frame" id="frame2">
            <property name="height">200</property>
            <property name="padding">5</property>
            <property name="width">200</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="padx">5</property>
              <property name="pady">5</property>
              <property name="row">2</property>
            </layout>
            <containerlayout manager="grid">
              <property type="col" id="all" name="weight">1</property>
              <property type="row" id="all" name="weight">1</property>
            </containerlayout>
            <child>
              <object class="ttk.Label" id="username_label" named="True">
                <property name="text" translatable="yes">Email Address or Username:</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">0</property>
                  <property name="sticky">e</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="password_label" named="True">
                <property name="text" translatable="yes">Password:</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">1</property>
                  <property name="sticky">e</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Entry" id="username_entry" named="True">
                <layout manager="grid">
                  <property name="column">1</property>
                  <property name="pady">5</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Entry" id="password_entry" named="True">
                <property name="show">â€¢</property>
                <layout manager="grid">
                  <property name="column">1</property>
                  <property name="pady">5</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="frame4">
        <property name="height">200</property>
        <property name="padding">5</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">2</property>
          <property name="sticky">ew</property>
        </layout>
        <containerlayout manager="grid">
          <property type="col" id="all" name="weight">1</property>
          <property type="row" id="all" name="weight">1</property>
        </containerlayout>
        <child>
          <object class="ttk.Button" id="login_submit" named="True">
            <property name="command" type="command" cbtype="simple">login_submit_button</property>
            <property name="text" translatable="yes">Submit</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
              <property name="sticky">w</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="login_cancel" named="True">
            <property name="command" type="command" cbtype="simple">login_cancel_button</property>
            <property name="text" translatable="yes">Cancel</property>
            <layout manager="grid">
              <property name="column">2</property>
              <property name="row">0</property>
              <property name="sticky">e</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
