<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="ttk.Frame" id="frame1">
    <layout manager="grid">
      <property name="column">0</property>
      <property name="row">0</property>
    </layout>
    <containerlayout manager="grid">
      <property type="col" id="0" name="weight">1</property>
      <property type="row" id="3" name="weight">1</property>
    </containerlayout>
    <child>
      <object class="ttk.Label" id="welcome_title">
        <property name="font">{Times New Roman} 16 {bold}</property>
        <property name="justify">center</property>
        <property name="text" translatable="yes">Welcome to the 
Panther Pantry Notification Tool!
</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">0</property>
          <property name="sticky">n</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="welcome_warning">
        <property name="font">{Times New Roman} 10 {underline}</property>
        <property name="text" translatable="yes">For Panther Pantry employee use only!
</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">1</property>
          <property name="sticky">n</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="welcome_frame">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">3</property>
        </layout>
        <containerlayout manager="grid">
          <property type="col" id="0" name="weight">1</property>
          <property type="row" id="0" name="weight">1</property>
        </containerlayout>
        <child>
          <object class="ttk.Label" id="welcome_instructions">
            <property name="font">{Times New Roman} 14 {}</property>
            <property name="justify">center</property>
            <property name="text" translatable="yes">Please select the "Send Notification", 
"Create Template", or 
"View Report" tab to begin
</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="frame3">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">4</property>
        </layout>
        <child>
          <object class="ttk.Label" id="error_text" named="True">
            <property name="font">{Times New Roman} 16 {bold}</property>
            <property name="justify">center</property>
            <property name="padding">5</property>
            <property name="text" translatable="yes">Not all tabs are available to all users!</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="frame2">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">5</property>
          <property name="sticky">sew</property>
        </layout>
        <containerlayout manager="grid">
          <property type="col" id="0" name="weight">1</property>
        </containerlayout>
        <child>
          <object class="ttk.Label" id="welcome_credit01">
            <property name="justify">left</property>
            <property name="text" translatable="yes">Spring 2023
PCC - 234A
</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
              <property name="sticky">sw</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Label" id="welcome_credit02">
            <property name="justify">right</property>
            <property name="text" translatable="yes">Written by:
Onxy Order of Programmers
</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">0</property>
              <property name="sticky">se</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
