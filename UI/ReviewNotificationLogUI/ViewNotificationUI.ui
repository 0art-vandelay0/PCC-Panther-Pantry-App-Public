<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="tk.Toplevel" id="view_notification_window" named="True">
    <property name="background">#e4e7e7</property>
    <property name="height">400</property>
    <property name="title" translatable="yes">View Notification</property>
    <property name="width">625</property>
    <child>
      <object class="tk.Frame" id="subject_frame" named="True">
        <property name="height">35</property>
        <property name="width">600</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="fill">both</property>
          <property name="side">top</property>
        </layout>
        <containerlayout manager="pack">
          <property name="propagate">False</property>
        </containerlayout>
        <child>
          <object class="tk.Label" id="subject_label" named="True">
            <property name="justify">left</property>
            <property name="text" translatable="yes">Subject:</property>
            <property name="width">7</property>
            <layout manager="pack">
              <property name="anchor">e</property>
              <property name="padx">5</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="subject_data_box" named="True">
            <property name="anchor">w</property>
            <property name="font">{work sans} 12 {}</property>
            <property name="justify">left</property>
            <property name="width">300</property>
            <layout manager="pack">
              <property name="padx">5</property>
              <property name="side">right</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="body_frame" named="True">
        <property name="height">100</property>
        <property name="width">605</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="fill">x</property>
          <property name="ipadx">5</property>
          <property name="side">bottom</property>
        </layout>
        <child>
          <object class="tk.Label" id="body_label" named="True">
            <property name="justify">right</property>
            <property name="text" translatable="yes">Body:</property>
            <property name="width">7</property>
            <layout manager="pack">
              <property name="anchor">ne</property>
              <property name="padx">5</property>
              <property name="pady">5</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Text" id="body_data_box" named="True">
            <property name="borderwidth">2</property>
            <property name="font">{work sans} 12 {}</property>
            <property name="padx">6</property>
            <property name="relief">ridge</property>
            <property name="width">75</property>
            <property name="wrap">word</property>
            <layout manager="pack">
              <property name="anchor">nw</property>
              <property name="side">top</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Frame" id="button_frame" named="True">
            <property name="height">60</property>
            <property name="width">200</property>
            <layout manager="pack">
              <property name="fill">x</property>
              <property name="side">top</property>
            </layout>
            <child>
              <object class="tk.Button" id="close_button" named="True">
                <property name="command" type="command" cbtype="simple">close_view</property>
                <property name="justify">center</property>
                <property name="padx">10</property>
                <property name="pady">2</property>
                <property name="text" translatable="yes">Close</property>
                <layout manager="pack">
                  <property name="padx">20</property>
                  <property name="pady">10</property>
                  <property name="side">right</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Button" id="select_all_button" named="True">
                <property name="command" type="command" cbtype="simple">select_all</property>
                <property name="justify">center</property>
                <property name="padx">10</property>
                <property name="pady">2</property>
                <property name="text" translatable="yes">Select All</property>
                <layout manager="pack">
                  <property name="anchor">w</property>
                  <property name="padx">20</property>
                  <property name="pady">10</property>
                  <property name="side">left</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="tk.Button" id="copy_button" named="True">
                <property name="command" type="command" cbtype="simple">copy</property>
                <property name="justify">center</property>
                <property name="padx">10</property>
                <property name="pady">2</property>
                <property name="text" translatable="yes">Copy</property>
                <layout manager="pack">
                  <property name="side">left</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="date" named="True">
        <property name="height">35</property>
        <property name="width">605</property>
        <layout manager="pack">
          <property name="expand">true</property>
          <property name="fill">both</property>
          <property name="side">top</property>
        </layout>
        <containerlayout manager="pack">
          <property name="propagate">False</property>
        </containerlayout>
        <child>
          <object class="tk.Label" id="date_label" named="True">
            <property name="justify">left</property>
            <property name="text" translatable="yes">Sent:</property>
            <property name="width">7</property>
            <layout manager="pack">
              <property name="anchor">e</property>
              <property name="padx">5</property>
              <property name="side">left</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Label" id="date_data_box" named="True">
            <property name="anchor">w</property>
            <property name="font">{work sans} 12 {}</property>
            <property name="justify">left</property>
            <property name="width">300</property>
            <layout manager="pack">
              <property name="padx">5</property>
              <property name="side">right</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
